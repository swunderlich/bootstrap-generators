<%- model_class = singular_table_name.camelcase.safe_constantize -%>
<%%= render partial: "shared/edit", locals: {model_class: <%=model_class %>, object: @<%= singluar_table_name%> } %>

<dl class="dl-horizontal">
  <%- attributes.each do |attribute| -%>
  <dt><%= attribute.human_name %>:</dt>
  <dd><%%= @<%= singular_table_name %>.<%= attribute.name %> %></dd>

  <%- end -%>
</dl>
